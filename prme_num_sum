import math
def is_prime(n):
    if n < 2:
        return False
    i = 2
    while(i >= 2 and i < (n/2) +1 ):
        if n % i == 0:
            i += 1
            return False
        else:
            continue
    return True

sum = 0

for i in range(102):
    if (is_prime(i) == True):
        print(i)
        sum += i

print(sum)